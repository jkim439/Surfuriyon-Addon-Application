<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="WindMinimap.lua"/>
	<Frame name="WindMinimap" parent="Minimap">
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
			<TileSize>
				<AbsValue val="16"/>
			</TileSize>
			<BackgroundInsets>
				<AbsInset left="5" right="5" top="5" bottom="5"/>
			</BackgroundInsets>
		</Backdrop>
		<Frames>
			<Frame name="WindMinimapBorderTop" inherits="WindBorder" hidden="false"/>
		</Frames>
		<Scripts>
			<OnLoad>WindMinimap_OnLoad();</OnLoad>
			<OnEvent>WindMinimap_OnEvent();</OnEvent>
			<OnMouseWheel>WindMinimap_OnMouseWheel(arg1);</OnMouseWheel>
		</Scripts>
	</Frame>
</Ui>
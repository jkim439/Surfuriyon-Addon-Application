<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="WindCastingConfig.lua"/>
	<Frame name="WindCastingConfigBorder" inherits="WindBorder" parent="UIParent" hidden="false"/>

	<Frame name="WindCastingConfig" inherits="WindBorder" parent="UIParent">
		<Size>
			<AbsDimension x="210" y="100" />
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT" relativeTo="WindConfig" relativePoint="TOPRIGHT">
				<Offset>
					<AbsDimension x="0" y="0" />
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>

			<Frame name="WindCastingConfigBody" inherits="WindLocationSize" hidden="false">
				<Size>
					<AbsDimension x="40" y="20" />
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="10" y="-10" />
						</Offset>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer>
						<FontString inherits="WindFontString" text="전체" justifyH="LEFT" hidden="false"/>
					</Layer>
				</Layers>
			</Frame>

		</Frames>
		<Scripts>
			<OnShow>WindCastingConfig_OnShow();</OnShow>
		</Scripts>
	</Frame>

</Ui>
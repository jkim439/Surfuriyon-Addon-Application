	<Frame name="WindChatFrame7" parent="ChatFrame7" inherits="WindChatBorder" hidden="false"/>
	<Frame name="WindChatEditBox" parent="ChatFrameEditBox" inherits="WindBorder" frameStrata="DIALOG" hidden="false">
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="WindChatEditBoxBG">
					<Color r="0.0" g="0.0" b="0.0" a="1.0"/>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>WindChatEditBox_OnLoad();</OnLoad>
			<OnEvent>WindChatEditBox_OnEvent();</OnEvent>
		</Scripts>
	</Frame>
</Ui>
